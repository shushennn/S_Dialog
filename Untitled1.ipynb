   "source": [
    "import sounddevice as sd\n",
    "import soundfile as sf \n",
    "sr = 44100\n",
    "duration = 5\n",
    "myrecording = sd.rec(int(duration * sr), samplerate=sr, channels=2)\n",
    "sd.wait()  \n",
    "sf.write(\"New Record.wav\", myrecording, sr)\n",
    "\n",
    "print(myrecording)"
   ]
